{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"jsx": "react-jsx",
		"module": "esnext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"resolveJsonModule": true,
		"target": "es2020",
		"paths": {
			"@/*": ["${configDir}/src/*"]
		}
	}
}
